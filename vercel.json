{
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-store, no-cache, must-revalidate, proxy-revalidate"
          },
          {
            "key": "Pragma",
            "value": "no-cache"
          },
          {
            "key": "Expires",
            "value": "0"
          },
          {
            "key": "Surrogate-Control",
            "value": "no-store"
          },
          {
            "key": "CDN-Cache-Control",
            "value": "no-store"
          },
          {
            "key": "Vercel-CDN-Cache-Control",
            "value": "no-store"
          }
        ]
      },
      {
        "source": "/api/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-store, no-cache, must-revalidate, proxy-revalidate"
          },
          {
            "key": "Pragma",
            "value": "no-cache"
          },
          {
            "key": "Expires",
            "value": "0"
          },
          {
            "key": "Surrogate-Control",
            "value": "no-store"
          },
          {
            "key": "CDN-Cache-Control",
            "value": "no-store"
          },
          {
            "key": "Vercel-CDN-Cache-Control",
            "value": "no-store"
          }
        ]
      }
    ],
    "routes": [
      {
        "source": "/api/(.*)",
        "destination": "/api/$1",
        "headers": {
          "Cache-Control": "no-store, no-cache, must-revalidate, proxy-revalidate",
          "CDN-Cache-Control": "no-store",
          "Vercel-CDN-Cache-Control": "no-store"
        }
      }
    ]
  }